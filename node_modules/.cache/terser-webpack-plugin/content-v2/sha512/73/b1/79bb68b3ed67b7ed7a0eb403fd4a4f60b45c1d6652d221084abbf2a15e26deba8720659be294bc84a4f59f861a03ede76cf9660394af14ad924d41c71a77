    <nav aria-label=\"Page navigation example\" className=\"pt-5\">\n      <ul className=\"pagination justify-content-center justify-content-lg-end\">\n        <li className=\"page-item\">\n          <button\n            className=\"page-link\"\n            onClick={() => onDownPage(page)}\n            disabled={page <= 1}\n          >\n            <span></span>\n          </button>\n        </li>\n        <IndexPage\n          indexPage={indexPage}\n          handlerChangePage={handlerChangePage}\n          pagination={pagination}\n        />\n        <li className=\"page-item\">\n          <button\n            className=\"page-link\"\n            onClick={() => onUpPage(page)}\n            disabled={page >= totalPage}\n          >\n            <span></span>\n          </button>\n        </li>\n      </ul>\n      <div className=\"pagination justify-content-center justify-content-lg-end\">\n        <p className=\"text-small text-muted mb-0\">1/{totalPage} Pages</p>\n      </div>\n    </nav>\n  );\n}\n\nexport default Pagination;\n","import React, { useEffect } from 'react';\nimport PropTypes from 'prop-types';\nimport { Link } from 'react-router-dom';\nimport convertMoney from '../../convertMoney';\n\nProducts.propTypes = {\n\tproducts: PropTypes.array,\n\tsort: PropTypes.string,\n};\n\nProducts.defaultProps = {\n\tproducts: [],\n\tsort: '',\n};\n\nfunction Products(props) {\n\tconst { products, sort } = props;\n\n\tif (sort === 'DownToUp') {\n\t\tproducts.sort((a, b) => {\n\t\t\treturn a.price - b.price;\n\t\t});\n\t} else if (sort === 'UpToDown') {\n\t\tproducts.sort((a, b) => {\n\t\t\treturn b.price - a.price;\n\t\t});\n\t}\n\n\treturn (\n\t\t<div className='row'>\n\t\t\t{/* -------------Product----------------- */}\n\t\t\t{products &&\n\t\t\t\tproducts.map((value) => (\n\t\t\t\t\t<div\n\t\t\t\t\t\tclassName='col-lg-4 col-sm-6 Section_Category'\n\t\t\t\t\t\tkey={value._id}>\n\t\t\t\t\t\t<div className='product text-center'>\n\t\t\t\t\t\t\t<div className='position